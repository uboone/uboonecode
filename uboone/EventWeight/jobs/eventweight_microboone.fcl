    type: XSecRatio
    histogram_file: "xsData/xsRatio/numu_nue_xsratio_2dhists.root"
    random_seed: 1
    parameter_list: ["Fv3", "Fa3"]
    parameter_sigma: [1, 1]
    mode: multisim
    number_of_multisims: 100
